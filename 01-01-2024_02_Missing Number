class Solution {
public:
    int missingNumber(vector<int>& nums) {
        int ArraySum=0;
        for(int i=0;i<nums.size();i++)
        ArraySum=ArraySum+nums[i];
        int n=nums.size();
        int expectedSum=(n*(n+1))/2;
        int MissingNum=expectedSum-ArraySum;
        return MissingNum; 
    }
};
